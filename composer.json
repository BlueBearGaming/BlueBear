{
  "name": "BlueBear",
  "license": "MIT",
  "type": "project",
  "description": "BlueBear, pandas against the world",
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:log0ymxm/bootswatch-scss.git"
    }
  ],
  "require": {
    "php": "~7.0",
    "symfony/symfony": "~3.0",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/assetic-bundle": "~2.7",
    "symfony/asset": "^3.0",
    "symfony/monolog-bundle": "^2.8",

    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-migrations-bundle": "^1.1",
    "stof/doctrine-extensions-bundle": "^1.2",

    "sensio/generator-bundle": "^3.0",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0",
    "twig/extensions": "^1.3",
    "incenteev/composer-parameter-handler": "^2.1",

    "oneup/uploader-bundle": "^1.5",


    "mopa/bootstrap-bundle": "3.*@dev",
    "twbs/bootstrap-sass": "3.*@dev",

    "clever-gaming/jikpoze": "*@dev",

    "packagist/yuicompressor-bin": "~2.4",
    "nc/elephantio-bundle": "~2.0",
    "hoa/ruler": "~2.0",
    "hautelook/alice-bundle": "^1.2",
    "letournel/path-finder": "^1.1",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "jms/security-extra-bundle": "^1.6",
    "jms/serializer-bundle": "^1.1",
    "lag/adminbundle": "dev-dev",
    "sidus/eav-model-bundle": "dev-symfony3"
  },
  "require-dev": {
    
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "component-dir": "web/assets"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "copy",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
